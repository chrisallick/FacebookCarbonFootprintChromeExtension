{
    "name": "Your Cloud Carbon Footprint",
    "description": "Calculate and show the energy you've spent browsing Facebook, Netflix, and YouTube.",
    "version": "0.1.0",
    "manifest_version": 2,
    "incognito": "split",
    "chrome_url_overrides": {
        "newtab": "blank.html"
    }, "background": {
        "scripts": ["background.js"],
        "persistent": true
    }, "permissions": [
        "tabs", "http://*/*", "https://*/*", "storage", "management"
    ], "browser_action": {
        "default_icon": "icon128.png"
    }, "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "64": "icon64.png",
        "128": "icon128.png"
    }
}